{
  "presets": [
    "stage-0",
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 10"],
          "node": "current"
        },
        "exclude": ["transform-regenerator"],
        "useBuiltIns": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "lodash",
    "fast-async",
    [
      "universal-import",
      {
        "disableWarnings": true
      }
    ],
    "react-hot-loader/babel"
  ],
  "ignore": "app.static.js",
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel", "fast-async"]
    }
  }
}
